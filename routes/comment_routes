const express = require("express");
const router = express.Router();
const comment_controller = require("../controllers/comment_controller");

router.get("/", comment_controller.comment_get_all);
router.get("/:id", comment_controller.comment_get_single);
router.post("/", comment_controller.comment_create);
router.patch("/:id", comment_controller.comment_patch);
router.delete("/:id", comment_controller.comment_delete);

module.exports = router;
